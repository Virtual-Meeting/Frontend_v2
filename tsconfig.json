{
  "compilerOptions": {
    "baseUrl": "src", 
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "*": ["*", "*.ts", "*.tsx"],
      "lib/*": ["lib/*"],
      "pages/*": ["pages/*"],
      "components/common/*":["components/common/*"],
      "assets/images/*": ["src/assets/images/*"],
      "assets/icons/*": ["assets/icons/*"]
    },
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*",
    "src/types/**/*.d.ts"
  ]
}
